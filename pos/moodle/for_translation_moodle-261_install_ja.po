msgstr "保留中のコースリクエスト"

#. courses
#: install/lang/en/admin.php:342
msgctxt "install/lang/en/admin.php:courses"
msgid "Courses"
msgstr "コース"

#. coursesperpage
#: install/lang/en/admin.php:343
msgctxt "install/lang/en/admin.php:coursesperpage"
